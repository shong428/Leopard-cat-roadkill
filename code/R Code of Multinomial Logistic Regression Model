library(ggpubr)
figure_all<- ggarrange(UB2019,UB2020,UB2021,UB2022,UB2023,
                       WT2019,WT2020,WT2021,WT2022,WT2023,
                       WL2019,WL2020,WL2021,WL2022,WL2023,
                       GR2019,GR2020,GR2021,GR2022,GR2023,
                       ncol=5,nrow=4)
figure_all      
figure_all<- ggarrange(UB2019,WT2019,WL2019,GR2019,
                       UB2020,WT2020,WL2020,GR2020,
                       UB2021,WT2021,WL2021,GR2021,
                       UB2022,WT2022,WL2022,GR2022,
                       UB2023,WT2023,WL2023,GR2023,
                       ncol=4,nrow=5)

figure_all
