range(datark$D.Urban)
range(datark$D.water)
range(datark$D.wetland)
range(datark$D.grass)

new_D.Urban <- rep(seq(-4.75, 2.63, length.out = 100),5)
new_D.water <- rep(seq(-3.91, 2.58, length.out = 100),5)
new_D.wetland <- rep(seq(-3.05, 2.21, length.out = 100),5)
new_D.grass <- rep(seq(-2.41, 3.40, length.out = 100),5)

Year <- rep( c("2019", "2020", "2021", "2022", "2023"), each = 100)

new_D.Urban <- rep(seq(-4.75, 2.94, length.out = 100),5)
new_D.water <- rep(seq(-3.91, 3.68, length.out = 100),5)
new_D.wetland <- rep(seq(-3.21, 2.21, length.out = 100),5)
new_D.grass <- rep(seq(-2.41, 4.53, length.out = 100),5)

Year2019 <- rep( c("2019"), each = 100)
Year2020 <- rep( c("2020"), each = 100)
Year2021 <- rep( c("2021"), each = 100)
Year2022 <- rep( c("2022"), each = 100)
Year2023 <- rep( c("2023"), each = 100)

new_data_D.Urban <- data.frame(D.Urban = new_D.Urban,
                               D.water = mean(datark$D.water),
                               D.wetland = mean(datark$D.wetland),
                               D.grass = mean(datark$D.grass),
                               Year = Year)
new_data_D.Urban$Year <- as.factor(new_data_D.Urban$Year)

new_data_D.water <- data.frame(D.Urban = mean(datark$D.Urban),
                               D.water = new_D.water,
                               D.wetland = mean(datark$D.wetland),
                               D.grass = mean(datark$D.grass),
                               Year = Year)
new_data_D.water$Year <- as.factor(new_data_D.water$Year)

new_data_D.wetland <- data.frame(D.Urban =mean(datark$D.Urban),
                                 D.water = mean(datark$D.water),
                                 D.wetland = new_D.wetland,
                                 D.grass = mean(datark$D.grass),
                                 Year = Year)
new_data_D.wetland$Year <- as.factor(new_data_D.wetland$Year)

new_data_D.grass <- data.frame(D.Urban = mean(datark$D.Urban),
                               D.water = mean(datark$D.water),
                               D.wetland = mean(datark$D.wetland),
                               D.grass = new_D.grass,
                               Year2019 = Year)
new_data_D.grass$Year <- as.factor(new_data_D.grass$Year)


predictions_D.Urban <- predict(ghsum,  type = "response", se.fit = TRUE, newdata= new_data_D.Urban)
predictions_D.water <- predict(ghsum,  type = "response", se.fit = TRUE, newdata= new_data_D.water)
predictions_D.wetland <- predict(ghsum,  type = "response", se.fit = TRUE, newdata= new_data_D.wetland)
predictions_D.grass <- predict(ghsum,  type = "response", se.fit = TRUE, newdata= new_data_D.grass)

fit_D.Urban <- as.data.frame(predictions_D.Urban$fit)
fit_D.water <- as.data.frame(predictions_D.water$fit)
fit_D.wetland <- as.data.frame(predictions_D.wetland$fit)
fit_D.grass <- as.data.frame(predictions_D.grass$fit)

se.fit_D.Urban <- as.data.frame(predictions_D.Urban$se.fit)
se.fit_D.water <- as.data.frame(predictions_D.water$se.fit)
se.fit_D.wetland <- as.data.frame(predictions_D.wetland$se.fit)
se.fit_D.grass <- as.data.frame(predictions_D.grass$se.fit)

lower_D.Urban <- fit_D.Urban - 1.96 * se.fit_D.Urban
upper_D.Urban <- fit_D.Urban + 1.96 * se.fit_D.Urban

lower_D.water <- fit_D.water - 1.96 * se.fit_D.water
upper_D.water <- fit_D.water + 1.96 * se.fit_D.water

lower_D.wetland <- fit_D.wetland - 1.96 * se.fit_D.wetland
upper_D.wetland <- fit_D.wetland + 1.96 * se.fit_D.wetland

lower_D.grass <- fit_D.grass - 1.96 * se.fit_D.grass
upper_D.grass <- fit_D.grass + 1.96 * se.fit_D.grass


names(fit_D.Urban) <- paste0("fit.", names(fit_D.Urban))
names(lower_D.Urban) <- paste0("lower.", names(lower_D.Urban))
names(upper_D.Urban) <- paste0("upper.", names(upper_D.Urban))

names(fit_D.water) <- paste0("fit.", names(fit_D.water))
names(lower_D.water) <- paste0("lower.", names(lower_D.water))
names(upper_D.water) <- paste0("upper.", names(upper_D.water))

names(fit_D.wetland) <- paste0("fit.", names(fit_D.wetland))
names(lower_D.wetland) <- paste0("lower.", names(lower_D.wetland))
names(upper_D.wetland) <- paste0("upper.", names(upper_D.wetland))

names(fit_D.grass) <- paste0("fit.", names(fit_D.grass))
names(lower_D.grass) <- paste0("lower.", names(lower_D.grass))
names(upper_D.grass) <- paste0("upper.", names(upper_D.grass))


pp.write_D.Urban <- cbind(new_data_D.Urban, fit_D.Urban, lower_D.Urban, upper_D.Urban)
pp.write_D.water <- cbind(new_data_D.water, fit_D.water, lower_D.water, upper_D.water)
pp.write_D.wetland <- cbind(new_data_D.wetland, fit_D.wetland, lower_D.wetland, upper_D.wetland)
pp.write_D.grass <- cbind(new_data_D.grass, fit_D.grass, lower_D.grass, upper_D.grass)



pp.write_D.Urban2019 <- subset(pp.write_D.Urban, Year == 2019)
pp.write_D.water2019 <- subset(pp.write_D.water, Year == 2019)
pp.write_D.wetland2019 <- subset(pp.write_D.wetland, Year == 2019)
pp.write_D.grass2019 <- subset(pp.write_D.grass, Year == 2019)



UB2019 <- ggplot(pp.write_D.Urban2019, aes(x = D.Urban)) +
  geom_line(aes(y = lower.NO), linetype = "dashed", color="coral") +  # lower에 해당하는 선
  geom_line(aes(y = upper.NO), linetype = "dashed", color="coral") +
  geom_line(aes(y = lower.AD), linetype = "dashed", color="chartreuse2") +  # lower에 해당하는 선
  geom_line(aes(y = upper.AD), linetype = "dashed", color="chartreuse2") +
  geom_line(aes(y = lower.SubAD), linetype = "dashed", color="deeppink1") +  # lower에 해당하는 선
  geom_line(aes(y = upper.SubAD), linetype = "dashed", color="deeppink1") +
  geom_line(aes(y = lower.JV), linetype = "dashed", color="darkgreen") +  # lower에 해당하는 선
  geom_line(aes(y = upper.JV), linetype = "dashed", color="darkgreen") +
  geom_line(aes(y = lower.UNID), linetype = "dashed", color="cyan3") +  # lower에 해당하는 선
  geom_line(aes(y = upper.UNID), linetype = "dashed", color="cyan3") +
  geom_line(aes(y=fit.NO), color="coral", linewidth = 2) + 
  geom_line(aes(y=fit.AD), color="chartreuse2", linewidth = 2) +
  geom_line(aes(y=fit.SubAD), color="deeppink1", linewidth = 2) +
  geom_line(aes(y=fit.JV), color="darkgreen", linewidth = 2) +
  geom_line(aes(y=fit.UNID), color="cyan3", linewidth = 2) +
  labs(x = "D.Urban", y = "2019") +  # 축과 범례 제목 설정
  theme_minimal()+ ylim(0,1)+ # 깔끔한 테마 적용
  theme_bw()+
  theme(
    legend.position = "right",
    axis.text = element_text(size = 8),
    axis.text.x = element_text(face = "bold", color = "black", size = 11),
    axis.text.y = element_text(face = "bold", color = "black", size = 11),
    axis.title.x = element_text(margin = margin(t = 20, r = 0, b = 0, l = 0), color = "black", size = 17, face = "bold"),
    axis.title.y = element_text(margin = margin(t = 20, r = 0, b = 0, l = 0), color = "black", size = 17, face = "bold"),
    panel.grid.major = element_blank(),
    panel.grid.minor = element_blank(),
    panel.background = element_blank(),
    axis.line = element_line(colour = "black")
  )                       
