datark$ad <- as.factor(datark$ad)
datark$lane <- as.factor(datark$lane)
datark$road <- as.factor(datark$road)
datark$fence <- as.factor(datark$fence)
datark$Year <- as.factor(datark$Year)


datark <- data.frame(datark)

datark_scaled <- scale(datark[, c("car", "bus", "small.struck",
                                  "midsize.truck", "large.truck", "sum", "D.Urban",
                                  "D.agri", "D.forest", "D.grass", "D.wetland", "D.bare", "D.water",
                                  "A.Urban", "A.Road", "A.agri", "A.forest", "A.Grass", "A.wetland", "A.bare", "A.water")])

#####################VIF

a <- vifstep(datark[,c("car", "bus", "small.struck", "midsize.truck", "large.truck", "sum", "D.Urban", "D.agri", "D.forest", "D.grass", "D.wetland", "D.bare", "D.water", "A.Urban", "A.Road", "A.agri", "A.forest", "A.Grass", "A.wetland", "A.bare", "A.water")], th=3)->VIF 
a
VIF@results$Variables

############total
gh <-  mblogit(ad ~  bus + large.truck + D.Urban + D.agri +D.forest  + D.grass+  D.wetland+
                 D.bare + D.water+ A.Urban + A.agri + A.Grass+ A.wetland +A.bare+  A.water+ lane + road + fence
               , random=c(~1|Cluster),data = datark)
gh <-  mblogit(ad ~  fence+ D.forest + bus + 
                 ,data = datark)
summary(gh)

calculate_AICc <- function(AIC, k, n) {
  AICc = AIC + (2 * k * (k + 1)) / (n - k - 1)
  return(AICc)
}

################################################################### multi-model 1
gh1 <-  mblogit(ad ~  bus + large.truck + D.Urban + D.agri + lane + A.agri+D.forest + fence, random=c(~1|Year),data = datark)
summary(gh1)
qq1 <- dredge(gh1, rank = "AIC")
n <- nrow(datark) 
qq1$AICc <- apply(qq1, 1, function(row) {
  k <- as.numeric(row['df']) 
  AIC <- as.numeric(row['AIC'])  
  calculate_AICc(AIC, k, n)
})
qq1$delta_AICc <- qq1$AICc - min(qq1$AICc)
rk1 <- get.models(qq1, subset = cumsum(delta_AICc) <= 2)
rk11 <- model.avg(rk1, fit = TRUE)
summary(rk11)

################################################################### multi-model 2

gh2 <- mblogit(ad ~  D.grass+  D.wetland+ D.bare + D.water+ A.Urban + fence + road , random=c(~1|Year),data = datark)
summary(gh2)
qq2 <- dredge(gh2, rank = "AIC")
n <- nrow(datark)  
qq2$AICc <- apply(qq2, 1, function(row) {
  k <- as.numeric(row['df']) 
  AIC <- as.numeric(row['AIC']) 
  calculate_AICc(AIC, k, n)
})
qq2$delta_AICc <- qq2$AICc - min(qq2$AICc)
rk2 <- get.models(qq2, subset = cumsum(delta_AICc) <= 2)
rk22 <- model.avg(rk2, fit = TRUE)
summary(rk22)

################################################################### multi-model 3

gh3 <-  mblogit(ad ~  A.Grass+ A.wetland +A.bare+  A.agri+D.forest + A.water, random=c(~1|Year),data = datark)
summary(gh3)
qq3 <- dredge(gh3, rank = "AIC")
n <- nrow(datark)  
qq3$AICc <- apply(qq3, 1, function(row) {
  k <- as.numeric(row['df'])  
  AIC <- as.numeric(row['AIC'])  
  calculate_AICc(AIC, k, n)
})
qq3$delta_AICc <- qq3$AICc - min(qq3$AICc)
rk3 <- get.models(qq3, subset = cumsum(delta_AICc) <= 2)
rk33 <- model.avg(rk3, fit = TRUE)

summary(rk33)

###################################################Final Model####################
datark$ad <- relevel(datark$ad, ref = "NO")
ghsum <-  mblogit(ad ~ D.Urban + D.water + D.wetland + D.grass,data = datark)

summary(ghsum)
qqsum<-dredge(ghsum, evaluate=T, trace=2, m.lim=c(1, 5))

row(qqsum)

rksum <- get.models(qqsum, subset = cumsum(delta) <= 2)

rksumsum <- model.avg(rksum, fit = TRUE)

summary(rksumsum)
summary(ghsum)



